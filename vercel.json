{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.2" }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://${VERCEL_URL}",
        "SESSION_DRIVER": "cookie",
        "LOG_CHANNEL": "stderr",
        "ASSET_URL": "https://${VERCEL_URL}",
        "DB_CONNECTION": "mysql" 
    }
}